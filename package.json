{
  "name": "@langtail/node",
  "version": "0.1.1",
  "description": "",
  "main": "./dist/LangtailNode.js",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">18"
  },
  "scripts": {
    "test": "vitest",
    "ts": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "tsup",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "openai",
    "langtail",
    "nlp",
    "ai",
    "natural language processing",
    "gpt-3",
    "gpt-4",
    "anthropic"
  ],
  "authors": [
    "Langtail <hi@langtail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.5",
    "dotenv": "^16.4.5",
    "nock": "14.0.0-beta.5",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "module": "./dist/LangtailNode.mjs",
  "types": "./dist/LangtailNode.d.ts",
  "exports": {
    ".": {
      "require": "./dist/LangtailNode.js",
      "import": "./dist/LangtailNode.mjs",
      "types": "./dist/LangtailNode.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "openai": "^4.33.0"
